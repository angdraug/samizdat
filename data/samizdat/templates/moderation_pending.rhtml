<div class="box">
  <div class="box-title"><%= @title %></div>
  <div class="box-content">
<table>
<thead><tr><th><%= _('Date') %></th><th><%= _('Resource') %></th></tr></thead>
<tbody>
<% dataset[page - 1].each do |r|
  resource = Resource.new(@request, r[:resource])%>
<tr><td><%= format_date(r[:action_date]) %></td><td><%= resource.short %><%= resource.buttons %></td></tr>
<% end %>
</tbody>
</table>
<% if foot != '' %>
<div class="foot"><%= foot %></div>
<% end %>
</div></div>

